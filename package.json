{
  "name": "eternalcodev3",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "bun run format:fix && bun run lint:fix && cross-env NODE_OPTIONS=--no-warnings=ExperimentalWarning payload generate:types && cross-env NODE_OPTIONS=--no-warnings=ExperimentalWarning payload generate:importmap && next build",
    "start": "next start",
    "lint": "biome lint app components hooks lib content",
    "lint:fix": "biome lint --write app components hooks lib content",
    "format": "biome check app components hooks lib content",
    "format:fix": "biome format --write app components hooks lib content",
    "lint:biome": "biome lint app components hooks lib content",
    "lint:biome:fix": "biome lint --write app components hooks lib content",
    "type-check": "tsc --noEmit",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "bun run generate:types:payload",
    "generate:types:payload": "cross-env NODE_OPTIONS=\"--no-deprecation --import=./ignore-styles.mjs --import=tsx\" payload generate:types"
  },
  "dependencies": {
